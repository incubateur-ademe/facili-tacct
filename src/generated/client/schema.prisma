generator client {
  provider        = "prisma-client-js"
  output          = "../src/generated/client"
  previewFeatures = ["multiSchema"]
  binaryTargets   = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("SCALINGO_POSTGRESQL_URL")
  schemas  = ["analytics", "databases", "postgis", "ressources"]
}

