generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("SCALINGO_POSTGRESQL_URL")
}

model clc_paris {
  pk           Int                      @id @default(autoincrement())
  class_2018   String?                  @db.VarChar
  shape_length Float?
  shape_area   Float?
  geometry     Unsupported("geometry (Polygon, 4326)")?

  @@index([geometry], map: "clc_paris_geometry_geom_idx", type: Gist)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model test {
  class_2018   String?                 @db.VarChar(256)
  Shape_Length Float?
  Shape_Area   Float?
  pk           Int                     @id(map: "test_clc_pkey")
  geometry     Unsupported("geometry")
}

model test2 {
  pk           Int                     @id @default(0)
  class_2018   String?                 @db.VarChar(256)
  Shape_Length Float?
  Shape_Area   Float?
  geometry     Unsupported("polygon")?
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)
}
