model Themes {
  id                Int         @id @default(autoincrement())
  theme             String?
  code              String      @default("75000")
  question_history  String?     @default("")
  createdAt         DateTime    @default(now())
  finished          Boolean     @default(false)
  report_downloaded Boolean     @default(false)
  questions         Questions[]
  @@index([id])
  @@map("theme")
}
model Questions {
  pk        Int      @id @default(autoincrement())
  theme_pk  Themes   @relation(fields: [themeId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  themeId   Int
  question  String?
  answer    String?
  createdAt DateTime @default(now())
  @@index([pk])
  @@map("questions")
}

model clc_paris {
  pk        Int      @id @default(autoincrement())
  class_2018  String @db.VarChar(256)
  shape_length   Float?
  shape_area  Float?
  geometry    String?
  @@index([pk])
  @@map("clc_paris")
}
