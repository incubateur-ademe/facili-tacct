generator client {
  provider      = "prisma-client"
  output        = "../src/generated/client"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgres"
  schemas  = ["analytics", "databases_v2", "postgis_v2", "public", "ressources"]
}

